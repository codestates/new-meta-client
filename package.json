{
  "name": "new-meta-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.3.15",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "apexcharts": "^3.26.0",
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "graphql": "^15.5.0",
    "react": "^17.0.2",
    "react-apexcharts": "^1.3.9",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "typed.js": "^2.0.12",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "config": {
    "BUCKETNAME": "new-meta.ga"
  },
  "scripts": {
    "sass": "node-sass -wr --source-map true src/styles/main.scss src/style.css",
    "node-sass": "node-sass",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "mkbucket": "aws s3 mb s3://$npm_package_config_BUCKETNAME",
    "rmbucket": "aws s3 rb s3://$npm_package_config_BUCKETNAME",
    "lsbucket": "aws s3 ls",
    "uploadObj": "aws s3 sync ./build s3://$npm_package_config_BUCKETNAME",
    "lsObj": "aws s3 ls s3://$npm_package_config_BUCKETNAME",
    "rmObj": "aws s3 rm s3://$npm_package_config_BUCKETNAME",
    "update": "npm run build && npm run uploadObj"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1"
  }
}
